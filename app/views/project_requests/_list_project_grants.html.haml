- partial_data_src_url = url_for(project_requests_path({:project_id => model.id, :skip_wrapper => true, :granted => 1}))
%div.relations{:class => 'project-grant-partial-span', 'data-src' => partial_data_src_url}
  = render :partial => 'project_requests/project_grant_list', :locals => {:models => (model.project_requests.where(:granted => true)), :project => model}