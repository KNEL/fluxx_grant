%ul.lines
  - if model.is_a? FipRequest
    %li= model.fip_title
  - else
    %li= model.org_name_text
  %li= dollars_format(model.granted? ? model.amount_recommended : model.amount_requested)
  %li= mdy_date_format(model.request_received_at)
  %li= model.program.name
  %li= model.grant_or_request_id
  %li= model.project_summary